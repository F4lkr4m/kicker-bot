{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "ES2022",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "ES2019"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node"
    ]
  }
}